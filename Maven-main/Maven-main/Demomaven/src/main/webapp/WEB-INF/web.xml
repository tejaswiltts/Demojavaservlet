<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>Loginpage</servlet-name>
  	<display-name>Loginpage</display-name>
  	<description></description>
  	<servlet-class>com.ltts.loginid.Loginpage</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Newusers</servlet-name>
  	<display-name>Newusers</display-name>
  	<description></description>
  	<servlet-class>com.ltts.Newusers</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>Loginpage</servlet-name>
  	<url-pattern>/Loginpage</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Newusers</servlet-name>
  	<url-pattern>/Newusers</url-pattern>
  </servlet-mapping>
</web-app>
